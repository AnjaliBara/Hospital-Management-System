<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Hospital Management System</display-name>
  <servlet>
  	<servlet-name>changedpassword</servlet-name>
  	<display-name>changedpassword</display-name>
  	<description></description>
  	<servlet-class>com.user.servlet.changedpassword</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DoctorPasswordChange</servlet-name>
  	<display-name>DoctorPasswordChange</display-name>
  	<description></description>
  	<servlet-class>com.doctor.servlet.DoctorPasswordChange</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditProfile</servlet-name>
  	<display-name>EditProfile</display-name>
  	<description></description>
  	<servlet-class>com.doctor.servlet.EditProfile</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateStatus</servlet-name>
  	<display-name>UpdateStatus</display-name>
  	<description></description>
  	<servlet-class>com.doctor.servlet.UpdateStatus</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddDoctor</servlet-name>
  	<display-name>AddDoctor</display-name>
  	<description></description>
  	<servlet-class>com.admin.servlet.AddDoctor</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateAppointmentStatus</servlet-name>
  	<display-name>UpdateAppointmentStatus</display-name>
  	<description></description>
  	<servlet-class>com.admin.servlet.UpdateAppointmentStatus</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>changedpassword</servlet-name>
  	<url-pattern>/changedpassword</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DoctorPasswordChange</servlet-name>
  	<url-pattern>/DoctorPasswordChange</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditProfile</servlet-name>
  	<url-pattern>/EditProfile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateStatus</servlet-name>
  	<url-pattern>/UpdateStatus</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddDoctor</servlet-name>
  	<url-pattern>/AddDoctor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateAppointmentStatus</servlet-name>
  	<url-pattern>/UpdateAppointmentStatus</url-pattern>
  </servlet-mapping>
</web-app>
